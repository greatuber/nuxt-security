{
  "name": "nuxt-security",
  "version": "0.10.1",
  "license": "MIT",
  "type": "module",
  "homepage": "https://nuxt-security.vercel.app",
  "description": "ðŸ›¡ Security Module for Nuxt based on OWASP Top 10 and Helmet",
  "repository": {
    "url": "https://github.com/Baroshem/nuxt-security"
  },
  "keywords": [
    "nuxt",
    "vue",
    "security",
    "owasp",
    "helmet",
    "basic-auth",
    "rate-limit",
    "xss",
    "cors"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
