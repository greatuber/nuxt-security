    "limiter": "^2.1.0",
    "memory-cache": "^0.2.0",
    "pathe": "^1.0.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "latest",
